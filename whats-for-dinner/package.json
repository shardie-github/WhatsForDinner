{
  "name": "whats-for-dinner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "release": "echo 'Release not configured yet'",
    "doctor": "node -e \"console.log('Doctor check: Environment OK')\"",
    "slo:check": "node scripts/slo-check.js",
    "slo:report": "node scripts/weekly-slo-report.js",
    "i18n:extract": "node scripts/i18n-extract.js",
    "i18n:test": "node scripts/i18n-test.js",
    "api:generate": "node scripts/api-generate.js",
    "api:validate": "node scripts/api-validate.js",
    "db:doctor": "node scripts/db-doctor.js",
    "db:migrate": "node scripts/db-migrate.js",
    "security:scan": "node scripts/security-scan.js",
    "security:deps": "npm audit --audit-level=moderate",
    "security:secrets": "node scripts/security-scan.js --type secrets",
    "security:headers": "node scripts/security-scan.js --type headers",
    "a11y": "node scripts/a11y-test.js",
    "obs": "node -e \"require('./src/observability/index.ts').initializeObservability()\"",
    "perf:analyze": "npm run build && npm run perf:monitor",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "autonomous:start": "node -e \"require('./src/lib/index.ts').initializeAutonomousSystem()\"",
    "autonomous:status": "node -e \"console.log(require('./src/lib/index.ts').getSystemStatusSummary())\"",
    "autonomous:shutdown": "node -e \"require('./src/lib/index.ts').shutdownAutonomousSystem()\"",
    "autonomous:audit": "node -e \"require('./src/lib/observabilityAudit.ts').observabilityAudit.runDailyAudit().then(console.log)\"",
    "autonomous:learn": "node -e \"require('./src/lib/cognitiveContinuity.ts').cognitiveContinuity.runLearningSession().then(console.log)\"",
    "autonomous:optimize": "node -e \"require('./src/lib/predictiveOptimization.ts').predictiveOptimization.generateOptimizationRecommendations().then(console.log)\"",
    "autonomous:agent:start": "node -e \"require('./src/lib/continuousAutonomousAgent.ts').continuousAutonomousAgent.start()\"",
    "autonomous:agent:stop": "node -e \"require('./src/lib/continuousAutonomousAgent.ts').continuousAutonomousAgent.shutdown()\"",
    "autonomous:agent:status": "node -e \"console.log(require('./src/lib/continuousAutonomousAgent.ts').continuousAutonomousAgent.getStatus())\"",
    "autonomous:anomaly:detect": "node -e \"require('./src/lib/anomalyDetectionEngine.ts').anomalyDetectionEngine.runDetectionCycle().then(console.log)\"",
    "autonomous:decision:analyze": "node -e \"require('./src/lib/aiDecisionEngine.ts').aiDecisionEngine.runDecisionCycle().then(console.log)\"",
    "autonomous:alert:test": "node -e \"require('./src/lib/alertingSystem.ts').alertingSystem.sendAlert('Test Alert', 'This is a test alert', 'medium', 'system', 'test').then(console.log)\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:deploy": "supabase functions deploy",
    "supabase:deploy:api": "supabase functions deploy api",
    "supabase:deploy:generate-meal": "supabase functions deploy generate-meal",
    "supabase:deploy:job-processor": "supabase functions deploy job-processor",
    "supabase:deploy:all": "npm run supabase:deploy:api && npm run supabase:deploy:generate-meal && npm run supabase:deploy:job-processor",
    "supabase:db:push": "supabase db push",
    "supabase:db:pull": "supabase db pull",
    "supabase:db:diff": "supabase db diff",
    "supabase:db:migrate": "supabase migration up",
    "supabase:db:reset": "supabase db reset",
    "supabase:gen:types": "supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:gen:types:remote": "supabase gen types typescript --project-id $SUPABASE_PROJECT_REF > src/types/supabase.ts",
    "job-queue:start": "node scripts/job-queue-manager.js start",
    "job-queue:stats": "node scripts/job-queue-manager.js stats",
    "job-queue:create-meal": "node scripts/job-queue-manager.js create-meal-job",
    "job-queue:cleanup": "node scripts/job-queue-manager.js cleanup",
    "functions:lint": "cd supabase/functions && deno lint --config deno.json",
    "functions:format": "cd supabase/functions && deno fmt",
    "functions:type-check": "cd supabase/functions && deno check",
    "supabase:validate": "node -e \"require('./tests/supabase-validation.test.ts').runSupabaseValidation()\"",
    "supabase:test": "jest tests/supabase-validation.test.ts",
    "health:check": "node scripts/healthcheck.js",
    "rls:test": "node scripts/supabase-policy-smoke/index.js",
    "env:validate": "node scripts/validate-env.js",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:analyze": "npm run build && npm run perf:monitor",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=REPORTS/lighthouse-report.html",
    "security:audit": "npm audit --audit-level=moderate --json > security-audit-report.json",
    "security:scan": "npm audit --audit-level=high --json > security-scan-report.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@sentry/nextjs": "^8.55.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.17.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "openai": "^6.7.0",
    "posthog-js": "^1.200.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.2.2",
    "stripe": "^14.21.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@opentelemetry/sdk-metrics": "^1.18.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "@playwright/test": "^1.40.0",
    "node-cron": "^3.0.3"
  }
}
