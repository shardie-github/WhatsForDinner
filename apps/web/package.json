{
  "name": "@whats-for-dinner/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "autonomous:start": "node -e \"require('./src/lib/index.ts').initializeAutonomousSystem()\"",
    "autonomous:status": "node -e \"console.log(require('./src/lib/index.ts').getSystemStatusSummary())\"",
    "autonomous:shutdown": "node -e \"require('./src/lib/index.ts').shutdownAutonomousSystem()\"",
    "autonomous:audit": "node -e \"require('./src/lib/observabilityAudit.ts').observabilityAudit.runDailyAudit().then(console.log)\"",
    "autonomous:learn": "node -e \"require('./src/lib/cognitiveContinuity.ts').cognitiveContinuity.runLearningSession().then(console.log)\"",
    "autonomous:optimize": "node -e \"require('./src/lib/predictiveOptimization.ts').predictiveOptimization.generateOptimizationRecommendations().then(console.log)\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@sentry/nextjs": "^8.55.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.17.0",
    "@whats-for-dinner/config": "workspace:*",
    "@whats-for-dinner/theme": "workspace:*",
    "@whats-for-dinner/ui": "workspace:*",
    "@whats-for-dinner/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "next-pwa": "^5.6.0",
    "openai": "^6.7.0",
    "posthog-js": "^1.200.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.2.2",
    "stripe": "^14.21.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
